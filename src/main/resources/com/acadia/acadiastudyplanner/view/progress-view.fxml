<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<!-- The 'stylesheets' attribute has been removed from this root element -->
<VBox spacing="20.0" styleClass="main-content" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.acadia.acadiastudyplanner.controller.ProgressController">
    <padding><Insets bottom="30.0" left="40.0" right="40.0" top="30.0" /></padding>
    <Label styleClass="header-title" text="My Progress" />
    <Label styleClass="header-subtitle" text="Visualize your study efforts and track your completion." />
    <VBox spacing="30.0" VBox.vgrow="ALWAYS">
        <Label styleClass="section-title" text="Subject Mastery" />
        <FlowPane fx:id="progressFlowPane" hgap="30" vgap="30" />
    </VBox>
</VBox>

